{
  "expo": {
    "name": "Awesome Chatapp",
    "description": "Showcase of an Expo app coded in TypeScript.",
    "slug": "Awesome-Chatapp",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#fff",
    "icon": "./src/assets/icons/app-icon.png",
    "loading": {
      "icon": "./src/assets/icons/loading-icon.png"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "android": {
      "package": "com.mcc.awesomechatapp",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "icon": "./src/assets/icons/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/app-icon.png",
        "backgroundColor": "#FFFFFF",
        "backgroundImage": "./src/assets/icons/app-icon.png"
      },
      "permissions": ["CAMERA",
        "READ_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "CAMERA_ROLL"]
    },
    "ios": {
      "bundleIdentifier": "com.mcc.awesomechatapp",
      "supportsTablet": true
    }
  }
}